{
  "name": "quick-ai",
  "type": "module",
  "version": "1.0.1",
  "description": "Quick AI SDK for easy AI integration",
  "url": "https://quickai.work",
  "repository": {
    "type": "git",
    "url": "https://github.com/quickaiwork/quick-ai-sdk.git"
  },
  "homepage": "https://github.com/quickaiwork/quick-ai-sdk#readme",
  "main": "dist/quick-ai-sdk.umd.js",
  "module": "dist/quick-ai-sdk.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/quick-ai-sdk.esm.js",
    "require": "./dist/quick-ai-sdk.umd.js",
    "default": "./dist/quick-ai-sdk.esm.js"
  },
  "scripts": {
    "build": "rollup -c",
    "prepare": "npm run build",
    "generate": "npx openapi-typescript-codegen --input https://api.aidirty.work/openapi --output src/generated"
  },
  "keywords": [
    "ai",
    "ai",
    "tools",
    "ai",
    "api",
    "ai",
    "moderation",
    "ai",
    "transformation"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "rollup": "^4.34.4",
    "typescript": "^5.7.3"
  }
}
